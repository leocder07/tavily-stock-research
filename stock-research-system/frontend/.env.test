# Test Environment Configuration (for E2E tests)
# This file is used automatically when running tests

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000

# Environment
REACT_APP_ENVIRONMENT=test

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_TEST_MODE=true  # Bypass authentication for E2E tests

# Clerk Authentication (disabled in test mode)
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_disabled_for_testing
VITE_CLERK_PUBLISHABLE_KEY=pk_test_disabled_for_testing

# Production Environment Configuration
# Copy this file to .env.production and replace placeholder values

# API Configuration - AWS Elastic Beanstalk Backend
REACT_APP_API_URL=https://your-eb-environment.us-east-1.elasticbeanstalk.com
REACT_APP_WS_URL=wss://your-eb-environment.us-east-1.elasticbeanstalk.com

# Environment
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false

# Optional: Custom Domain (if configured)
# REACT_APP_API_URL=https://api.yourdomain.com
# REACT_APP_WS_URL=wss://api.yourdomain.com

# Build Configuration
# Generate sourcemaps: false for production (smaller bundle)
GENERATE_SOURCEMAP=false
